#: usr/lib/linuxmint/mintnanny/mintnanny.py:22
msgid "Please type the domain name you want to block"
msgstr "Voer a.u.b. de domeinnaam in die u wil blokkeren"

#: usr/lib/linuxmint/mintnanny/mintnanny.py:23
msgid "Domain name"
msgstr "Domeinnaam"

#: usr/lib/linuxmint/mintnanny/mintnanny.py:45
msgid "Invalid Domain"
msgstr "Ongeldig domein"

#: usr/lib/linuxmint/mintnanny/mintnanny.py:47
#, python-format
msgid "%s is not a valid domain name."
msgstr "%s is geen geldige domeinnaam."

#: usr/lib/linuxmint/mintnanny/mintnanny.py:48
msgid ""
"Domain names must start and end with a letter or a digit, and can only "
"contain letters, digits, dots and hyphens."
msgstr ""
"Domeinnamen moeten beginnen en eindigen met een letter of een cijfer, en "
"kunnen uitsluitend letters, cijfers, punten en koppeltekens bevatten."

#: usr/lib/linuxmint/mintnanny/mintnanny.py:49
msgid "Example: my.number1domain.com"
msgstr "Bijvoorbeeld: mijn.nummer1domein.nl"

#: usr/lib/linuxmint/mintnanny/mintnanny.py:107 generate_desktop_files:25
#: generate_desktop_files:39
msgid "Domain Blocker"
msgstr "Domeinblokkeerder"

#: usr/lib/linuxmint/mintnanny/mintnanny.py:113
msgid "Blocked domains"
msgstr "Geblokkeerde domeinen"

#: generate_desktop_files:25 generate_desktop_files:39
msgid "Block access to selected domain names"
msgstr "Blokkeer toegang tot geselecteerde domeinnamen"

#: generate_desktop_files:39
msgid "Parental Control"
msgstr "Ouderlijk toezicht"

