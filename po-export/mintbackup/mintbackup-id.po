#, python-format
msgid "Your files were successfully saved in %s."
msgstr ""

#: usr/lib/linuxmint/mintbackup/mintbackup.py:505
msgid ""
"Warning: The meta file could not be saved. This backup will not be accepted "
"for restoration."
msgstr ""

#: usr/lib/linuxmint/mintbackup/mintbackup.py:532
#, python-format
msgid ""
"Warning: Some files were not saved. Only %(archived)d files were backed up "
"out of %(total)d."
msgstr ""

#: usr/lib/linuxmint/mintbackup/mintbackup.py:547
msgid "Restoring:"
msgstr "Menyimpan ulang:"

#: usr/lib/linuxmint/mintbackup/mintbackup.py:564
msgid "The restoration was aborted."
msgstr ""

#: usr/lib/linuxmint/mintbackup/mintbackup.py:568
msgid "Your files were successfully restored."
msgstr ""

#: usr/lib/linuxmint/mintbackup/mintbackup.py:656
#, python-format
msgid "Warning: Only %(number)d files were restored out of %(total)d."
msgstr ""

